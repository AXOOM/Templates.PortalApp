{
  "ClientId": "myvendor-myapp",
  "ClientName": "My App",
  "RequireConsent": false,
  "AllowedGrantTypes": [
    "implicit"
  ],
  "AllowAccessTokensViaBrowser": true,
  "RedirectUris": [
    "",
    "/silentRenew.html"
  ],
  "AllowedScopes": [
    "openid",
    "profile",
    "myvendor-myapp.api"
  ]
}
, {
  "ClientId": "myvendor-myapp-swagger",
  "ServiceNameHostPart": "myvendor-myapp",
  "ClientSecrets": [ "${MYVENDOR_MYAPP_IDENTITY_CLIENT_SECRET:-myvendor-myapp-identity-client-secret}" ],
  "ClientName": "Swagger",
  "AccessTokenType": "Jwt",
  "AllowedGrantTypes": [
    "implicit"
  ],
  "AllowAccessTokensViaBrowser": true,
  "RedirectUris": [
    "/swagger/o2c.html"
  ],
  "AllowedScopes": [
    "openid",
    "profile",
    "email",
    "myvendor-myapp.api"
  ]
}
