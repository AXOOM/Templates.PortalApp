  "AllowedGrantTypes": [
    "implicit"
  ],
  "AllowAccessTokensViaBrowser": true,
  "RedirectUris": [
    "${PUBLIC_PROTOCOL:-https}://vendorname-appname-${PUBLIC_DOMAIN}",
    "${PUBLIC_PROTOCOL:-https}://vendorname-appname-${PUBLIC_DOMAIN}/silentRenew.html"
  ],
  "AllowedScopes": [
    "openid",
    "profile",
    "email",
    "vendorname-appname.api"
  ],
