{
  "ClientId": "vendorname-appname",
  "ClientName": "My App",
  "RequireConsent": false,
  "AllowedGrantTypes": [
    "implicit"
  ],
  "AllowAccessTokensViaBrowser": true,
  "RedirectUris": [
    "",
    "/silentRenew.html"
  ],
  "AllowedScopes": [
    "openid",
    "profile",
    "vendorname-appname.api"
  ]
}
, {
  "ClientId": "vendorname-appname-swagger",
  "ClientSecrets": [ "${VENDORNAME_APPNAME_IDENTITY_CLIENT_SECRET:-vendorname-appname-identity-client-secret}" ],
  "ClientName": "Swagger",
  "AccessTokenType": "Jwt",
  "AllowedGrantTypes": [
    "implicit"
  ],
  "AllowAccessTokensViaBrowser": true,
  "RedirectUris": [
    "/swagger/o2c.html"
  ],
  "AllowedScopes": [
    "openid",
    "profile",
    "email",
    "vendorname-appname.api"
  ]
}
